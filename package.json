{
  "name": "nocms-editor",
  "version": "0.0.1",
  "description": "Text editor for NoCMS",
  "main": "lib/index.js",
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org/nocms/nocms-editor.git"
  },
  "scripts": {
    "dev": "webpack --watch --inline",
    "test": "",
    "prepublish": "babel ./src --out-dir ./lib --source-maps --presets es2015,react --plugins babel-plugin-add-module-exports,babel-plugin-transform-class-properties && browserify ./lib/index.js -o ./build/nocms-editor.js --transform browserify-global-shim --standalone NoCMSEditor && uglifyjs ./build/nocms-editor.js --compress --mangle --output ./build/nocms-editor.min.js --source-map ./build/nocms-editor.min.js.map"
  },
  "dependencies": {
    "draft-js": "^0.9.0",
    "draft-js-export-html": "^0.4.0",
    "draft-js-import-html": "^0.2.0",
    "nocms-events": "git+ssh://git@bitbucket.org/nocms/nocms-events.git"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "browserify": "^13.0.1",
    "browserify-global-shim": "^1.0.3",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "uglify-js": "^2.6.4",
    "watch": "^0.19.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "peerDependencies": {
    "react": "^15.0.0"
  },
  "browserify-global-shim": {
    "react": "React",
    "react-dom": "ReactDOM"
  }
}
